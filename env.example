# AWS Configuration
# Required: Your AWS credentials for S3 access
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_REGION=ap-south-1
S3_BUCKET_NAME=your-bucket-name
S3_FILE_KEY=indigo_reviews.csv

# Model Configuration
# Optional: Default values are provided in the code
MODEL_ID=google/flan-t5-base
EMBEDDING_MODEL=all-MiniLM-L6-v2

# RAG Configuration
# Optional: Fine-tune retrieval parameters
CHUNK_SIZE=400
CHUNK_OVERLAP=50
TOP_K_RETRIEVAL=3

# Generation Parameters
# Optional: Adjust response generation
MAX_RESPONSE_TOKENS=150
MIN_RESPONSE_TOKENS=40
TEMPERATURE=0.4
REPETITION_PENALTY=2.5

# System Configuration
# Optional: Customize file paths
FEEDBACK_FILE=user_feedback.json
